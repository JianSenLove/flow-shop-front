<view class="container"><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" circular="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{homeList.lb}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="carousel-item"><image src="{{item.url}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="cate-section"><block wx:for="{{LabelsList}}" wx:for-item="itme" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToList',['$0',0],[[['LabelsList','',index,'id']]]]]]]}}" class="cate-item" bindtap="__e"><image src="{{itme.ioc}}"></image><text>{{itme.name}}</text></view></block></view><block wx:if="{{homeList.zx.url!=''}}"><view class="ad-1"><image src="{{homeList.zx.url}}" mode="scaleToFill" data-event-opts="{{[['tap',[['xiangq']]]]}}" bindtap="__e"></image></view></block><view class="hot-floor"><view class="f-header m-t"><image src="/static/temp/h1.png"></image><view class="tit-box"><text class="tit">新品上市</text><text class="tit2">New arrival</text></view></view><scroll-view class="floor-list" scroll-x="{{true}}"><view class="scoll-wrapper"><block wx:for="{{xList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['xList','',index,'id']]]]]]]}}" class="floor-item" bindtap="__e"><image src="{{item.ioc}}" mode="aspectFill"></image><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+item.price}}</text></view></block><view data-event-opts="{{[['tap',[['navToList',[0,1]]]]]}}" class="more" bindtap="__e"><text>查看全部</text><text>More+</text></view></view></scroll-view></view><view class="f-header m-t"><image src="/static/hot.png"></image><view class="tit-box"><text class="tit">热销推荐</text><text class="tit2">Best Selling Recommendation</text></view><text data-event-opts="{{[['tap',[['navToList',[0,2]]]]]}}" class="yticon icon-you" bindtap="__e"></text></view><view class="guess-section"><block wx:for="{{sList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['sList','',index,'id']]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.ioc}}" mode="aspectFill"></image></view><text class="title clamp">{{item.title}}</text><text class="price">{{"￥"+(item.price||0)}}</text></view></block></view></view>